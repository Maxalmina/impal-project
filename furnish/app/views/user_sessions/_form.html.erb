<%= form_with(local: true) do |form| %>
  <div class="input-field">
    <%= form.label :username %>
    <%= form.text_field :username %>
  </div>
  <div class="input-field">
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div class="actions">
    <%= form.button "Login", type: :submit, class: "btn waves-effect waves-light blue darken-2 white-text"  %>
  </div>
<% end %>