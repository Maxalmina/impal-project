<% content_for :title do %>
    Dashboard
    <% if content_for?(:page_title) %>
      | <%= yield :page_title %>
    <% end %>
<% end %>
  


<% content_for :content do %>
  <%= yield :page_header %>
    <div id="main">

        <!-- START WRAPPER -->

        <div class="wrapper">

        <!-- START CONTENT -->
        <%= yield :page_content %>
        <!-- END CONTENT -->

        </div>

        <!-- END WRAPPER -->
        
    </div>
  <% end %>

<%= render template: "layouts/application" %>